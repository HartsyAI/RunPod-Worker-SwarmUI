# SwarmUI RunPod Serverless Environment Variables

# SwarmUI Server Configuration
SWARMUI_HOST=0.0.0.0
SWARMUI_PORT=7801

# Network Volume Configuration
VOLUME_PATH=/runpod-volume

# Handler Configuration
SWARMUI_API_URL=http://127.0.0.1:7801
GENERATION_TIMEOUT=600
SERVICE_WAIT_INTERVAL=5
STARTUP_TIMEOUT=900

# Optional: Auto-download models on first start
AUTO_DOWNLOAD_MODELS=false

# Optional: Default model to use if not specified
DEFAULT_MODEL=OfficialStableDiffusion/sd_xl_base_1.0